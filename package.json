{
  "name": "goosetrack",
  "version": "0.0.1",
  "description": "Mongoose plugin to keep track of specific properties updates",
  "main": "index.js",
  "repository": "https://github.com/ndelvalle/goosetrack.git",
  "author": "ndelvalle <nicolas.delvalle@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js",
    "format": "prettier --write '**/*.{js,json,md}'",
