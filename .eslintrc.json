{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-use-before-define": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-continue": 0,
    "no-shadow": 0
  },
  "parserOptions": {
    "ecmaVersion": 2018
  }
}
